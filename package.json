{
  "name": "flash-books",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "cross-env EXPO_NO_DOTENV=1 ENVFILE=.env expo run:android",
    "expo:clean": "expo start -c",
    "android:build:apk": "cross-env EXPO_NO_DOTENV=1 ENVFILE=.env android/gradlew assembleRelease -p android",
    "android:build:staging": "cross-env EXPO_NO_DOTENV=1 ENVFILE=.env.staging android/gradlew bundleRelease -p android",
    "android:build:staging:apk": "cross-env EXPO_NO_DOTENV=1 ENVFILE=.env.staging android/gradlew assembleRelease -p android",
    "android:build:production": "cross-env EXPO_NO_DOTENV=1 ENVFILE=.env.production android/gradlew bundleRelease -p android",
    "android:clean": "android/gradlew clean -p android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc",
    "postinstall": "patch-package",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@expo-google-fonts/comfortaa": "^0.2.3",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "axios": "^1.7.3",
    "deprecated-react-native-prop-types": "^5.0.0",
    "expo": "^53.0.17",
    "expo-font": "~13.3.2",
    "expo-status-bar": "~2.2.3",
    "jwt-decode": "^4.0.0",
    "luxon": "^3.5.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-iap": "^12.16.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-snap-carousel": "^3.9.1",
    "toastify-react-native": "^7.2.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/luxon": "^3.4.2",
    "@types/react": "~19.0.10",
    "@types/react-native-snap-carousel": "^3.8.11",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^5.0.0",
    "patch-package": "^8.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
